</span><span class='line'><span class="o">&gt;</span>         <span class="n">exif</span> <span class="o">=</span> <span class="s2">&quot;</span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Make&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2"> </span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Model&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2"> </span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Lens Model&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2"> </span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Exposure&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2">sec f/</span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Aperture&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2">  ISO</span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;ISO Speed&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2"> Focal length:</span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Focal Length&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2">(</span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Focal Length (35mm format)&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2">) on </span><span class="si">#{</span><span class="n">ef</span><span class="o">[</span><span class="s1">&#39;Date and Time (Original)&#39;</span><span class="o">]</span><span class="si">}</span><span class="s2">&quot;</span>
</span><span class='line'><span class="o">&gt;</span>
</span><span class='line'><span class="mi">47</span><span class="n">c50</span>
</span><span class='line'><span class="o">&lt;</span>         <span class="s2">&quot;&lt;a class=</span><span class="se">\&quot;</span><span class="s2">thumbnail</span><span class="se">\&quot;</span><span class="s2"> href=</span><span class="se">\&quot;</span><span class="si">#{</span><span class="vi">@photo</span><span class="o">[</span><span class="ss">:url</span><span class="o">]</span><span class="si">}</span><span class="se">\&quot;</span><span class="s2">&gt;&lt;img src=</span><span class="se">\&quot;</span><span class="si">#{</span><span class="n">selected_size</span><span class="o">[</span><span class="ss">:source</span><span class="o">]</span><span class="si">}</span><span class="se">\&quot;</span><span class="s2"> title=</span><span class="se">\&quot;</span><span class="si">#{</span><span class="vi">@photo</span><span class="o">[</span><span class="ss">:title</span><span class="o">]</span><span class="si">}</span><span class="se">\&quot;</span><span class="s2">&gt;&lt;/a&gt;&quot;</span>
</span><span class='line'><span class="o">---</span>
</span><span class='line'><span class="o">&gt;</span>         <span class="s2">&quot;&lt;a class=</span><span class="se">\&quot;</span><span class="s2">thumbnail</span><span class="se">\&quot;</span><span class="s2"> href=</span><span class="se">\&quot;</span><span class="si">#{</span><span class="vi">@photo</span><span class="o">[</span><span class="ss">:url</span><span class="o">]</span><span class="si">}</span><span class="se">\&quot;</span><span class="s2">&gt;&lt;img src=</span><span class="se">\&quot;</span><span class="si">#{</span><span class="n">selected_size</span><span class="o">[</span><span class="ss">:source</span><span class="o">]</span><span class="si">}</span><span class="se">\&quot;</span><span class="s2"> title=</span><span class="se">\&quot;</span><span class="si">#{</span><span class="vi">@photo</span><span class="o">[</span><span class="ss">:title</span><span class="o">]</span><span class="si">}</span><span class="se">\&quot;</span><span class="s2">&gt;&lt;/a&gt;&lt;br/&gt;&lt;small&gt;</span><span class="si">#{</span><span class="n">exif</span><span class="si">}</span><span class="s2">&lt;/small&gt;&quot;</span>
